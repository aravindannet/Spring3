<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

<bean name="sequenceGeneratorusingvalueattribute"  class="com.training.SequenceGenerator">
	<constructor-arg value="10000" />
	<constructor-arg value="30" />
	<constructor-arg value="A" />

</bean>
<bean name="sequenceGeneratorusingvaluetag"  class="com.training.SequenceGenerator">
	<constructor-arg><value>100000</value></constructor-arg>
	<constructor-arg> <value>30</value></constructor-arg>
	<constructor-arg><value>A</value></constructor-arg>

</bean>

	
</beans>